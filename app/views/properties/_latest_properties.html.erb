<% @properties.each do |property| %>
<div class="col-3">
	<%= link_to (image_tag property.photo_url, class:'photo-thumbnail'), property_path(property) if property.photo_url %>
	<h3 class="mt-3"><%= property.name %></h3>
	<p><%= property.address %></p>
	<div class="amenities">
		<span><i class="fas fa-bed"></i></span>
		<span><i class="fas fa-bath"></i></span>
		<span><i class="fas fa-person-booth"></i></span>
	</div>
</div>
<% end %>